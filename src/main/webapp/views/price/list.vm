<div class="row">
	<div class="col-lg-12" >
		<form class="form-horizontal" action="${rc.contextPath}/adminOrder/orders" method="post">
			<div class="col-lg-1" style="font-size:20px;width:12%">
				查询条件
			</div>
			<div class="col-lg-10">
				<div class="form-group">
					<label class="col-lg-2 control-label">渠道用户ID</label>
				    <div class="col-lg-3"><input class="form-control" type="text" name="userId" value="$!userId"></div>
				    <div class="col-lg-3">
				    	<button type="submit" class="btn btn-primary btn-large">查询</button>
				    </div>
			    </div>
		    </div>
		</form>
	</div>
</div>
<div class="row admin-row-margin">
	<hr>
	<div class="col-lg-12">
		<table class="table table-striped table-bordered table-hover">
			<thead style="background-color:#0099CC;">
				<th>渠道用户ID</th>
				<th>卡+名</th>
				<th>卡+证</th>
				<th>卡+证+名</th>
				<th>卡+证+名+机</th>
				<th>操作</th>
			</thead>
			<tbody>
				#foreach($chnlPrice in $!chnlPriceList)
				<tr>
					<td>$!chnlPrice.userId</td>
					<td>$!chnlPrice.mode1Price</td>
					<td>$!chnlPrice.mode2Price</td>
					<td>$!chnlPrice.mode3Price</td>
					<td>$!chnlPrice.mode4Price</td>
					<td><a href="#">修改</a>  <a href="#">删除</a></td>
				</tr>
				#end
			</tbody>
		</table>
		<!-- 分页-->
		#if($!page.total > $!page.pageSize)
			<div class="row ">
				<div style="float:right">
				<ul class="pagination">
					<li><a href="${rc.contextPath}/adminOrder/orders?limit=1&orderId=$!repayOrder.orderId&orderSt=$!repayOrder.orderSt&repayUserNm=$!repayOrder.repayUserNm&creditCardAcctNo=$!repayOrder.creditCardAcctNo&startTime=$!startTime&endTime=$!endTime">首页</a></li>
	              #if($!page.currNo!=1)
	                  <li class="previous"><a  href="${rc.contextPath}/adminOrder/orders?limit=$!page.lastNo&orderId=$!repayOrder.orderId&orderSt=$!repayOrder.orderSt&repayUserNm=$!repayOrder.repayUserNm&creditCardAcctNo=$!repayOrder.creditCardAcctNo&startTime=$!startTime&endTime=$!endTime" >上一页</a><li>
	              #end
	            #set($no=[$!page.beginPgNo .. $!page.nextEndPgNo])
	            #foreach($i in $no) 
	                <li #if(($!page.currNo)==$i) class="active" #else class="disable" #end><a href="${rc.contextPath}/adminOrder/orders?limit=$i&orderId=$!repayOrder.orderId&orderSt=$!repayOrder.orderSt&repayUserNm=$!repayOrder.repayUserNm&creditCardAcctNo=$!repayOrder.creditCardAcctNo&startTime=$!startTime&endTime=$!endTime" >$i</a></li>
	            #end
	             #if($!page.currNo!=$!page.endPgNo)
	                <li class="next"><a href="${rc.contextPath}/adminOrder/orders?limit=$!page.nextNo&orderId=$!repayOrder.orderId&orderSt=$!repayOrder.orderSt&repayUserNm=$!repayOrder.repayUserNm&creditCardAcctNo=$!repayOrder.creditCardAcctNo&startTime=$!startTime&endTime=$!endTime" >下一页</a></li>
	             #end
	             	<li><a href="${rc.contextPath}/adminOrder/orders?limit=$page.endPgNo&orderId=$!repayOrder.orderId&orderSt=$!repayOrder.orderSt&repayUserNm=$!repayOrder.repayUserNm&creditCardAcctNo=$!repayOrder.creditCardAcctNo&startTime=$!startTime&endTime=$!endTime" class="home">尾页</a></li>
	             	<li><a >共$!page.pageNo页</a></li>
	            </ul>
	            </div>
	        </div>
		#end
		<!--分页 End -->
	</div>
</div>

